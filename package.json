  "private": true,
  "description": "White-label live entertainment experience platform",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:tokens": "eslint . --config .eslintrc.design-tokens.js",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:api": "vitest run tests/api",
